:Modifiedrankinscale2 a skos:Concept ;
    skos:inScheme :EmergencyNote ;
    skos:prefLabel "Modified rankin scale (Concept)"@en ;
    dc:description "Modified rankin scale" ;     

    skos:narrower :rankin0;
    skos:narrower :rankin1;
    skos:narrower :rankin2;
    skos:narrower :rankin3;
    skos:narrower :rankin4;
    skos:narrower :rankin5.

:rankin0
    skos:inScheme :EmergencyNote ;
    skos:prefLabel "0 - Keine Symptome"@en ;
    
    skos:notation "Rankin:0" .

:rankin1
    skos:inScheme :EmergencyNote ;
    skos:prefLabel "1 - Keine relevante Beeintraechtigung"@en ;
   
    skos:notation "Rankin:1" .

:rankin2
    skos:inScheme :EmergencyNote ;
    skos:prefLabel "2 - Leichte Beeintraechtigung"@en ;
  
    skos:notation "Rankin:2" .

:rankin3
