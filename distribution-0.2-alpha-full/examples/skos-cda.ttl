    skos:prefLabel "3 - Mittelschwere Beeintraechtigung"@en ;
  
    skos:notation "Rankin:3" .

:rankin4
    skos:inScheme :EmergencyNote ;
    skos:prefLabel "4 - Hoehergradige Beeintraechtigung"@en ;
   
    skos:notation "Rankin:4" .

:rankin5
    skos:inScheme :EmergencyNote ;
    skos:prefLabel "5 - Schwere Behinderung"@en ;

    skos:notation "Rankin:5" .


