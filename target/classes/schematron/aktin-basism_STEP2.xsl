                 test="not($set_other[@root=$root][empty($extension)] or $set_other[@root=$root][@extension=$extension])">dtr1-12-II: [RIM-002] <value-of select="$name"/> SHALL be distinct</assert>
    
         
         <let name="UUIDpattern"
              value="'^[0-9a-fA-F]{8}-[0-9a-fA-f]{4}-[0-9a-fA-F]{4}-[0-9a-aA-F]{4}-[0-9a-fA-F]{12}$'"/>
         <assert role="error"
                 test="not(@root) or not(matches(@root,$UUIDpattern) and matches(@root,'[a-z]'))">dtr1-13-II: When UUID is used, the hexadecimal digits A-F SHALL be in upper case</assert>
      </rule>
      
      <rule abstract="true" id="II.NL.BSN">
